<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
 
<channel>
  <title>Le Bobsch Blogsch</title>
  <link>http://lebobs.ch/</link>
  <description>Le Bobsch Blogs</description>
  <language>en-US</language>
  <copyright>Florian Holzner</copyright>
  <pubDate>Mon, 27 Jun 2011 22:04:00 +0200</pubDate>
  <atom:link href="http://lebobs.ch/rss.xml" rel="self" type="application/rss+xml" />
 
  {% for post in site.posts %}
  <item>
    <title>{{ post.title }}</title>
      <description>{{ post.teaser }}</description>
      <link>http://lebobs.ch{{ post.url }}</link>
      <author>bobschi@lavabit.com (Florian Holzner)</author>
      <guid>http://lebobs.ch{{ post.url }}</guid>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S +0100" }}</pubDate>
    </item>
  {% endfor %}
  </channel>
 
</rss>
